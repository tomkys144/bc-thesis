{
    "cSpell.enableFiletypes": [
        "optex"
    ],
    "cSpell.ignoreWords": [
        "arara",
        "begtt",
        "costanzo",
        "criswell",
        "endtt",
        "eqmark",
        "filtfreq",
        "glref",
        "hecht",
        "hfil",
        "lccc",
        "montht",
        "optex",
        "picw",
        "secc",
        "tankisi",
        "zayia",
        "ifcase"
    ],
    "cSpell.words": [
        "Aktin",
        "aktinem",
        "analergický",
        "bioinformatika",
        "branchiální",
        "butterworthův",
        "CMRR",
        "elektrodiagnostické",
        "extenzor",
        "extenzoru",
        "extenzorů",
        "extenzory",
        "extrafuzální",
        "filamentu",
        "filamentů",
        "flexoru",
        "flexorů",
        "flexory",
        "GETA",
        "Holter",
        "invaginovány",
        "karpálního",
        "marjoriehecht",
        "myofibrila",
        "myofibrily",
        "myofirbrila",
        "myosinem",
        "myosinová",
        "myosinové",
        "myosinovou",
        "myosinových",
        "myosinu",
        "neuroanatomy",
        "Neuromuskulární",
        "neuromuskulárního",
        "Nyquistův",
        "polymerizován",
        "pronaci",
        "repetitivních",
        "repetitivními",
        "retikula",
        "retikulem",
        "retikulu",
        "sarkopazmatickým",
        "tropomyosin",
        "tropomyosinem",
        "tropomyosinu",
        "troponin",
        "troponinu",
        "tubul",
        "ulnaris",
        "ventrální",
        "viscerální"
    ],
    "editor.wordWrap": "wordWrapColumn",
    "files.exclude": {
        "logseq/*": true,
        "notes/": true
    },
    "libraryFilePath": "references.bib",
    "[optex]": {
        "editor.defaultFormatter": "tomkys144.optex-formatter",
        "editor.formatOnSave": true
    },
    "todohighlight.include": [
        "**/**.tex"
    ],
    "todohighlight.keywords": [
        {
            "text": "TODO:",
            "regex": {
                "pattern": "(?<=^|\"|\\s)\\\\rfc{TODO:?[^}]*}"
            },
            "color": "#0042d5",
            "backgroundColor": "#ffbd2a",
            "overviewRulerColor": "rgba(255,189,42,0.8)"
        },
        {
            "text": "FIXME:",
            "regex": {
                "pattern": "(?<=^|\"|\\s)\\\\rfc{FIXME:?[^}]*}"
            },
            "color": "#0f9d6d",
            "backgroundColor": "#f06292",
            "overviewRulerColor": "rgba(240,98,146,0.8)"
        },
        {
            "text": "REVIEW",
            "regex": {
                "pattern": "(?<=^|\"|\\s)\\\\rfc{REVIEW:?[^}]*}"
            }
        },
        {
            "text": "NOTE",
            "regex": {
                "pattern": "(?<=^|\"|\\s)\\\\rfc{NOTE:?[^}]*}"
            },
            "backgroundColor": "#bc009b"
        },
        {
            "text": "DONE",
            "regex": {
                "pattern": "(?<=^|\"|\\s)\\\\rfc{DONE:?[^}]*}"
            },
            "backgroundColor": "#b8ffbe"
        }
    ]
}
